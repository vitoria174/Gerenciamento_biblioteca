class Biblioteca:
    def __init__(self):
        self.Lista_livro = []
        self.id_livro = 0
        
    def cadastrar_livro(self, titulo, autor, ano, genero, quantidade):
        self.id_livro += 1 
        titulolivro = titulo
        autorlivro = autor
        anolivro = ano
        generolivro = genero
        quantidadelivro = quantidade
        
        self.Lista_livro.append ({"NÂ° Cadastro":self.id_livro,"Titulo":titulo, "Autor":autor, "Ano": ano, "Genero":genero, "Quantidade":quantidade})
        
    def listar_livros(self):
        print(self.Lista_livro)
        
u = Biblioteca()
u.cadastrar_livro('alquimista','paulo coellho', '2001', 'fantasia', 4)
u.cadastrar_livro('livro1','autor1 ','2010', 'romance', 5)
u.listar_livros()
        
